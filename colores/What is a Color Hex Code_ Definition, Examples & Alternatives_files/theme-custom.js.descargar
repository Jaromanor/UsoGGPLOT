function guide_load_more(){
	jQuery('.action-moreguide').click(function(){
				  var dataoffset = jQuery(this).attr('dataoffset');
				  var datashow = jQuery(this).attr('datashow');
				  var datatag = jQuery(this).attr('data-tag');
				  var setofset = parseInt(dataoffset) + parseInt('10');
				  var datasite = jQuery(this).attr('datasite');
				  jQuery('.action-moreguide').attr('dataoffset',setofset);
				  var ajaxurl = datasite +'/wp-admin/admin-ajax.php';
				 jQuery.ajax({
					url:  ajaxurl,
					data: {'action':'load_moreguides', 'dataoffset': dataoffset,'datatag': datatag}, 
					type: 'post',  
					beforeSend: function(){
					jQuery('.action-moreguide').css('opacity', '0.5');
					},        	
					success: function (data) {
						var response = JSON.parse(data);
						if(response.success){
							jQuery('.topic-articles-load').append(response.html);
							jQuery('.action-moreguide').css('opacity', 1);
							jQuery('.action-moreguide').attr('dataoffset',setofset);
						}else{
							jQuery('.topic-articles-load').append(response.html);
							jQuery('.action-moreguide').hide();
						}  
					}   
				});  
			});
}
function openslot(evt, slotName) {
  var i, tabcontent, tablinks;
  tabcontent = document.getElementsByClassName("slot-leftinner-content");
  for (i = 0; i < tabcontent.length; i++) {
    tabcontent[i].style.display = "none";
  }
  tablinks = document.getElementsByClassName("slot-tablinks");
  for (i = 0; i < tablinks.length; i++) {
    tablinks[i].className = tablinks[i].className.replace(" active", "");
  }
  document.getElementById(slotName).style.display = "block";
  evt.currentTarget.className += " active";
}
