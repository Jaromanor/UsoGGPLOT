jQuery(document).find('.review_article_action').click(function(){
	var datasite = jQuery(this).attr('datasite');
	var datashow = jQuery(this).attr('datashow');
	var dataoffset = jQuery(this).attr('dataoffset');
	var dataAuthorID = jQuery(this).attr('dataAuthorID');
	var beforeSend = jQuery(this).attr('beforeSend');
	var afterSend = jQuery(this).attr('afterSend');
	var ajaxurl = datasite +'/wp-admin/admin-ajax.php';
	jQuery.ajax({
		url:  ajaxurl,
		data: {
		  'action':'review_article_Filter', 
		  'datashow': datashow,
		  'dataoffset': dataoffset,
		  'dataAuthorID': dataAuthorID,
		}, 
		type: 'post',  
		beforeSend: function(){
			jQuery('.review_article_action').text(beforeSend);
			jQuery('.review_article_action').css('opacity', '0.5');
		},
		success: function (data) {
			var res = JSON.parse(data);
            var totalCount = res.total_count;
			if(res.html=='No More Articles'){
					jQuery('.review_article_action').hide();
				}
			if(res.success){
				
					jQuery('.articles-leftinner').append(res.html);
					jQuery('.review_article_action').text(afterSend);
					jQuery('.review_article_action').css('opacity', 1);
					var setofset = parseInt(dataoffset) + 10;
					jQuery('.review_article_action').attr("dataoffset",setofset);
					if(totalCount < 10){
						jQuery('.review_article_action').hide();
					}
				
			}
		}  
	  });  
});
