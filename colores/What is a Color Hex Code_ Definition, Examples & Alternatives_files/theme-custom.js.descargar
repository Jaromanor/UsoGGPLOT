  jQuery('#jan-filter-inputTopics').on('change', function(){
    var catID = jQuery(this).val() ? jQuery(this).val() : '';
    var authorID = jQuery('#jan-filter-inputExperts').val() ? jQuery('#jan-filter-inputExperts').val() : '';
    var datasite = jQuery(this).attr('data-site');
    var ajaxurl = datasite +'/wp-admin/admin-ajax.php';
    jQuery.ajax({
      url:  ajaxurl,
      data: {
        'action':'getFilteredQA', 
        'catID': catID,
        'authorID': authorID,
      }, 
      type: 'post',  
      beforeSend: function(){
        jQuery('.action-load-more-qa').hide();
        jQuery('.ajax-loading').show();
        jQuery('.filteredQA').html('');
      },        
      success: function (data) {
        jQuery('.ajax-loading').hide();
        var response = JSON.parse(data);
        if(response.found_posts <= 20){
          jQuery('.action-load-more-qa').hide();
        }
        if(response.found_posts > 0){
          jQuery('.action-load-more-qa').show();
          jQuery('.filteredQA').html(response.html);
        }else{
            jQuery('.action-load-more-qa').hide();
           jQuery('.filteredQA').html('No results found!');
        }
      }     
    });  
  })
